mixin navigation-list(options)
  -
    const {
      title,
      items
      } = options

  ul.navigation-list
    li(class=`navigation-list__item navigation-list__item_title`)= title
    each val in items
      li(class='navigation-list__item')
        a(class='navigation-list__item-link' href='/mock-address/change-me')= val
