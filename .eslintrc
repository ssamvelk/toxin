{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "func-names": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-undef": 1,
    "no-multiple-empty-lines": [2, {"max": 2, "maxEOF": 0}],
    "class-methods-use-this": 0
  }
}